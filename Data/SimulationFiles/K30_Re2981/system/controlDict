FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../config"

application     simpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         $endTime;

deltaT          1;

writeControl    timeStep;

writeInterval   $writeInt;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    #includeFunc "residuals"
    #includeFunc "stressComponents"

}

